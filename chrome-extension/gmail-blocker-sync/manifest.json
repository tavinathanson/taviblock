{
  "name": "Gmail Blocker Sync",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Blocks Gmail based on CLI tool status.",
  "permissions": [
    "nativeMessaging",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://mail.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
