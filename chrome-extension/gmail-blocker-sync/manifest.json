{
  "name": "Gmail Blocker Sync",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Blocks Gmail based on CLI tool status.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://mail.google.com/*",
    "nativeMessaging"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
